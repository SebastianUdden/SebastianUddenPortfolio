    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="A portfolio page">
    <meta name="author" content="Sebastian Uddén">
    <!-- #endregion -->
    <!-- #region AngularJS, jQuery and other js-->
    <script src="https://code.angularjs.org/1.5.6/angular.min.js"></script>
    <script src="https://code.angularjs.org/1.5.6/angular-route.min.js"></script>
    <script src="https://code.angularjs.org/1.5.6/angular-resource.min.js">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
    <script src="~/js/jquery.js"></script>
    <script src="~/js/bootstrap.js"></script>
    <script src="~/js/jquery.easing.min.js"></script>
    
    <script src="~/js/grayscale.js"></script>
    <!-- #endregion -->
    <!-- #region Bootstrap -->
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>

    <!-- #endregion -->
    <!-- #region Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Lato:400,100,100italic,300,300italic,400italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>
    <link href="http://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic" rel="stylesheet" type="text/css">
    <link href="http://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
    <link href="~/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <!-- #endregion -->
    <!-- #region My CSS -->
    <link href="~/css/navbar.css" rel="stylesheet" />
    <link href="~/css/images.css" rel="stylesheet" />
    <link href="~/CSS/margins.css" rel="stylesheet" />
    <link href="~/css/bootstrap.css" rel="stylesheet" />
    <link href="~/css/grayscale.css" rel="stylesheet" />
    <link href="~/CSS/main.css" rel="stylesheet" />
    <!-- #endregion -->
    <!-- #region Angular start-up -->
    <script>
        var app = angular.module('portfolioApp', ['ngRoute', 'ngResource']);
    </script>
    <!-- #endregion -->
</head>
